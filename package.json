{
  "name": "playup",
  "version": "0.4.0",
  "description": "Publish APKs to Google Play Store",
  "main": "index.js",
  "bin": {
    "playup": "cli.js"
  },
  "scripts": {
    "prepublish": "npm run compile && npm prune",
    "build": "node_modules/.bin/babel src --modules common --out-dir lib",
    "watch": "node_modules/.bin/babel src --watch --modules common --out-dir lib --source-maps true",
    "test": "node_modules/.bin/standard && node test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeduan/google-play-publisher"
  },
  "keywords": [
    "android",
    "google",
    "play",
    "apk"
  ],
  "author": "Jeduan Cornejo <jeduan@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeduan/google-play-publisher/issues"
  },
  "homepage": "https://github.com/jeduan/google-play-publisher",
  "dependencies": {
    "async": "^1.3.0",
    "debug": "^2.1.3",
    "googleapis": "^2.0.2",
    "node-apk-parser": "^0.2.3",
    "nodeify": "^1.0.0",
    "yargs": "^3.7.2"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "faucet": "0.0.1",
    "proxyquire": "^1.6.0",
    "sinon": "^1.15.4",
    "standard": "^4.5.4",
    "tape": "^4.0.0"
  }
}
